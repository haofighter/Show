<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.myself.show.show.Ui.home.fragment.HomeFragment">

    <com.myself.show.show.View.Twink.TwinklingRefreshLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <android.support.v7.widget.RecyclerView
            android:id="@+id/note_date_list"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"></android.support.v7.widget.RecyclerView>

    </com.myself.show.show.View.Twink.TwinklingRefreshLayout>
</FrameLayout>
